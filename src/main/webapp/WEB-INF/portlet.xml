<?xml version="1.0" encoding="UTF-8"?>

<portlet-app
    xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd"
    version="1.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd
                        http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd">

    <portlet>
        <portlet-name>NitecoEmployeeManagement</portlet-name>
        <display-name>Niteco employees management</display-name>
        <portlet-class>org.springframework.web.portlet.DispatcherPortlet</portlet-class>
		
        <supports>
            <mime-type>text/html</mime-type>
            <portlet-mode>view</portlet-mode>
            <portlet-mode>config</portlet-mode>
        </supports>
        
        <portlet-info>
            <title>Niteco employees management</title>
            <keywords>interactive</keywords>
        </portlet-info>
        
        <portlet-preferences>
			<preference>
				<name>listEmployee</name>
				<value>null</value>
			</preference>
		</portlet-preferences>
    </portlet>
    
    <portlet>
        <portlet-name>NitecoEmployeeChart</portlet-name>
        <display-name>Niteco employees chart</display-name>
        <portlet-class>org.springframework.web.portlet.DispatcherPortlet</portlet-class>
		
        <supports>
            <mime-type>text/html</mime-type>
            <portlet-mode>view</portlet-mode>
        </supports>
        <portlet-info>
            <title>Niteco employees chart</title>
            <keywords>interactive</keywords>
        </portlet-info>
        
        <portlet-preferences>
			<preference>
				<name>text</name>
				<value></value>
			</preference>
		</portlet-preferences>
    </portlet>
    <!-- 
    <custom-portlet-mode>
		<portlet-mode>config</portlet-mode>
	</custom-portlet-mode> -->
</portlet-app>
