<?xml version="1.0" encoding="UTF-8"?>

<portlet-app
    xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd"
    version="1.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd
                        http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd">

    <portlet>
        <portlet-name>NitecoEmployeePortlet</portlet-name>
        <display-name>Niteco Employees Portlet</display-name>
        <portlet-class>org.springframework.web.portlet.DispatcherPortlet</portlet-class>
		<!-- <expiration-cache>0</expiration-cache>
		<cache-scope>private</cache-scope> -->
        <supports>
            <mime-type>text/html</mime-type>
            <portlet-mode>VIEW</portlet-mode>
            <portlet-mode>CONFIG</portlet-mode>
        </supports>
        <!--
        <init-param>
			<name>contextConfigLocation</name>
            <value>/WEB-INF/NitecoEmployeePortlet-portlet.xml</value>
		</init-param>
		-->
        <portlet-info>
            <title>Niteco Employees</title>
            <keywords>interactive</keywords>
        </portlet-info>
        
        <portlet-preferences>
			<preference>
				<name>text</name>
				<value></value>
			</preference>
		</portlet-preferences>
    </portlet>
    
    <portlet>
        <portlet-name>NitecoCitiesPortlet</portlet-name>
        <display-name>Niteco Cities</display-name>
        <portlet-class>org.springframework.web.portlet.DispatcherPortlet</portlet-class>
		<!--<expiration-cache>60</expiration-cache>
		<cache-scope>private</cache-scope>-->
        <supports>
            <mime-type>text/html</mime-type>
            <portlet-mode>VIEW</portlet-mode>
        </supports>
         <!--
		<init-param>
			<name>contextConfigLocation</name>
            <value>/WEB-INF/NitecoCitiesPortlet-portlet.xml</value>
		</init-param>		
        -->
        <portlet-info>
            <title>Niteco Cities</title>
            <keywords>interactive</keywords>
        </portlet-info>
        
        <portlet-preferences>
			<preference>
				<name>text</name>
				<value></value>
			</preference>
		</portlet-preferences>
    </portlet>
    
    <custom-portlet-mode>
		<portlet-mode>config</portlet-mode>
	</custom-portlet-mode>
</portlet-app>
